let secretMessage = ['Learning', 'is', 'not', 'about', 'what', 'you', 'get', 'easily', 'the', 'first', 'time,', 'it', 'is', 'about', 'what', 'you', 'can', 'figure', 'out.', '-2015,', 'Chris', 'Pine,', 'Learn', 'JavaScript'];
// Removing the last element of the array.
secretMessage.pop()
//checking the log should be one element less
console.log(secretMessage.length);
//adding to more element to the end of the string.
secretMessage.push('to','Program');
//changing the work 'easyli' for another element to right
secretMessage[7]='right';
//removing the first element of the array.
secretMessage.shift();
// adding to the beginning of the string array programing
secretMessage.unshift('programming');
//removing some get.right,the,first,time from string and know
secretMessage.splice(6,10,'know');
//printing the the message in the
console.log(secretMessage.join(' '))
